<div class="container mt-5">
    <div class="row">
        <div class="col s12 m6 16">
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Nombre</th>
                        <th scope="col">Descripcion</th>
                    </tr>
                </thead>
                <tbody>
                    <% categorias.forEach(item=> { %>
                    <tr>
                        <td>
                            <%= item.id%>
                        </td>
                        <td>
                            <%= item.nombre %>
                        </td>
                        <td>
                            <%= item.descripcion %>
                        </td>
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>