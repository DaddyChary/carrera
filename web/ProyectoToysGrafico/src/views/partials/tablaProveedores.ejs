
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nombre</th>
            <th scope="col">Telefono</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        <% proveedores.forEach(item=> { %>
        <tr>
            <td>
                <%= item.id%>
            </td>
            <td>
                <%= item.nombre %>
            </td>
            <td>
                <%= item.telefono %>
            </td>
            <td>
                <%= item.email %>
            </td>
        </tr>
        <% }) %>
    </tbody>
</table>
